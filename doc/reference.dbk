                    <computeroutput xmlns:xi="http://www.w3.org/2001/XInclude"><phrase role="identifier">PyTypeObject</phrase> <phrase role="keyword">const</phrase> <phrase role="special">*</phrase>
                    <phrase role="identifier">p</phrase> <phrase role="special">=</phrase>
