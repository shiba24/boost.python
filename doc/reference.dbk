<programlisting xmlns:xi="http://www.w3.org/2001/XInclude"><phrase role="identifier">unspecified</phrase> <phrase role="keyword">operator</phrase><phrase role="special">()</phrase> <phrase role="keyword">const</phrase><phrase role="special">;</phrase>
