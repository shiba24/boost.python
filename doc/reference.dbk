<programlisting xmlns:xi="http://www.w3.org/2001/XInclude"><phrase role="keyword">using</phrase> <phrase role="keyword">namespace</phrase> <phrase role="identifier">boost</phrase><phrase role="special">::</phrase><phrase role="identifier">python</phrase><phrase role="special">;</phrase>
<phrase role="identifier">tuple</phrase> <phrase role="identifier">head_and_tail</phrase><phrase role="special">(</phrase><phrase role="identifier">object</phrase> <phrase role="identifier">sequence</phrase><phrase role="special">)</phrase>
